    'bunyanLog.R'
    'bunyanOnload.R'
    'bunyanSetLog.R'
    'bunyanSetpoint.R'
    'bunyanTraceback.R'
    'bunyanTracebackN.R'
    'bunyanLog.debug.R'
    'bunyanLog.error.R'
    'bunyanLog.fatal.R'
    'bunyanLog.info.R'
    'bunyanLog.trace.R'
    'bunyanLog.warn.R'
    'bunyanBuffer.R'
    'bunyanClearSetpoint.R'
    'bunyanStopLog.R'
    'bunyanTracebackErrors.R'
    'bunyanTracebackWarnings.R'
